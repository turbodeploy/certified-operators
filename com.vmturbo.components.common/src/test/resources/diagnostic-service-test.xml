<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.1.xsd">

    <context:annotation-config />

    <!-- the object under test: DiagnosticService -->
    <bean id="diagnosticService" class="com.vmturbo.components.common.diagnostics.DiagnosticService"/>

    <bean id="theComponent" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.vmturbo.components.common.IVmtComponent"/>
    </bean>

    <bean id="mockProcessFactory" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.vmturbo.components.common.OsProcessFactory"/>
    </bean>

    <bean id="mockCommandProcessRunner" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.vmturbo.components.common.OsCommandProcessRunner"/>
    </bean>

    <bean id="mockFileFolderZipper" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.vmturbo.components.common.diagnostics.FileFolderZipper"/>
    </bean>

    <bean name="testconfig" class="com.vmturbo.components.common.diagnostics.PropertyTestConfiguration"/>

</beans>
