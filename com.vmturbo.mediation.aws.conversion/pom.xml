<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <artifactId>xl-sdk-probe-parent</artifactId>
        <groupId>com.vmturbo</groupId>
        <version>7.11.0-SNAPSHOT</version>
        <relativePath>../com.vmturbo.mediation.probe.parent/pom.xml</relativePath>
    </parent>

    <packaging>jar</packaging>
    <artifactId>com.vmturbo.mediation.aws.conversion</artifactId>
    <description>Mediation Component for AWS conversion probe</description>

    <properties>
        <swagger.component.name>AWS Conversion Mediation Component</swagger.component.name>
    </properties>

    <build>
        <plugins>
            <plugin>
                <groupId>com.vmturbo</groupId>
                <artifactId>vmturbo-sdk-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

    <dependencies>
        <dependency>
            <groupId>com.vmturbo</groupId>
            <artifactId>com.vmturbo.mediation.cloud.conversion</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.vmturbo</groupId>
            <artifactId>com.vmturbo.mediation.aws</artifactId>
            <version>${vmturbo.mt.branch.version}</version>
        </dependency>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.mockito</groupId>
            <artifactId>mockito-core</artifactId>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.hamcrest</groupId>
            <artifactId>hamcrest-all</artifactId>
            <scope>test</scope>
        </dependency>
    </dependencies>

</project>
