        {
          "format": "Auto",
          "queryText": "select\r\n  ts_sum(\r\n    relabel(\r\n      ts_upsample(\r\n        ts_mask(\r\n          to_timerange(\r\n            end_time, \r\n            end_time\r\n            )\r\n          ) *-1 * cast(coalesce(tags ->> '$.commodity_deltas.Virtual Memory', tags ->> '$.commodity_deltas.Virtual CPU') as decimal), '24h'),\r\n        IF(tags ->> '$.commodity_deltas.Virtual Memory' IS NOT NULL, 'Virtual Memory', 'Virtual CPU'))),\r\n  IF(tags ->> '$.commodity_deltas.Virtual Memory' IS NOT NULL, 'Virtual Memory', 'Virtual CPU') as 'Delta Type'\r\nFROM\r\n  events\r\nWHERE\r\n  tags ->> '$.type' = 'Resize'\r\n  and tags ->> '$.state' in (${environment:sqlstring})\r\n  and NOT (\r\n    tags ->> '$.commodity_deltas.Virtual CPU' IS NULL AND tags ->> '$.commodity_deltas.Virtual Memory' IS NULL\r\n  )  \r\nGROUP BY IF(tags ->> '$.commodity_deltas.Virtual Memory' IS NOT NULL, 'Virtual Memory', 'Virtual CPU')",
          "refId": "A"
        }
      ],
      "timeFrom": null,
      "timeShift": null,
      "title": "VPU and Memory Savings Over Time",
      "type": "timeseries"
