        {
          "format": "Auto",
          "queryText": "SELECT\r\n      t.cluster as Cluster,\r\n      t.name as VM,\r\n      e.tags ->> '$.description' as Description,\r\n      IF(e.tags ->> '$.commodity_deltas.Virtual Memory' is not null, (e.tags ->> '$.commodity_deltas.Virtual Memory' / (1024*1024)) * ${cost_per_gb:sqlstring}, 0 ) as 'VMem Cost',\r\n      IF(e.tags ->> '$.commodity_deltas.Virtual CPU' is not null, (e.tags ->> '$.commodity_deltas.Virtual CPU') * ${cost_per_core:sqlstring}, 0 ) as 'VCPU Cost'\r\n  FROM\r\n      events as e\r\n  inner join (\r\n      SELECT\r\n        vm.xid as xid,\r\n        clus.name as cluster,\r\n        vm.name as name\r\n      FROM\r\n        GRAPH('(clus:Cluster)-[:has_member]->(pm:PhysicalMachine)-[:supports]->(vm:VirtualMachine)')\r\n  ) t on e.tags ->> '$.associated_xid' = t.xid\r\nWHERE\r\n  e.tags ->> '$.type' = 'Resize' and\r\n  e.tags ->> '$.state' in (${environment:sqlstring})\r\n",
          "refId": "A"
        }
      ],
