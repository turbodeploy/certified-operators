<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <localRepository>${user.home}/.m2/repository</localRepository>
  <interactiveMode>false</interactiveMode>
  <usePluginRegistry/>
  <offline>false</offline>
  <pluginGroups/>
  <mirrors/>
  <proxies/>
  <profiles>
    <profile>
      <id>vmturbo</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <repositories>
        <repository>
          <id>central</id>
          <name>vmturbo</name>
          <url>http://build.vmturbo.com:8081/nexus/content/repositories/vmturbo</url>
          <layout>default</layout>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>never</updatePolicy>
            <checksumPolicy>warn</checksumPolicy>
          </snapshots>
        </repository>
        <repository>
          <id>maven2 mirror</id>
          <name>maven2 mirror</name>
          <url>http://repo2.maven.org/maven2</url>
          <layout>default</layout>
        </repository>
        <repository>
          <id>snapshots</id>
          <name>snapshots</name>
          <url>http://build.vmturbo.com:8081/nexus/content/repositories/snapshots</url>
          <layout>default</layout>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>interval:60</updatePolicy>
            <checksumPolicy>warn</checksumPolicy>
          </snapshots>
        </repository>
        <repository>
          <id>repository typesafe</id>
          <name>typesafe repository</name>
          <url>http://repo.typesafe.com/typesafe/maven-releases</url>
        </repository>

      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>vmturbo</id>
          <name>Turbonomic Repository</name>
          <url>http://build.vmturbo.com:8081/nexus/content/repositories/vmturbo</url>
        </pluginRepository>
        <pluginRepository>
          <id>snapshots</id>
          <name>snapshots</name>
          <url>http://build.vmturbo.com:8081/nexus/content/repositories/snapshots</url>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
        <pluginRepository>
          <id>thirdparty</id>
          <name>Turbonomic Repository</name>
          <url>http://build.vmturbo.com:8081/nexus/content/repositories/thirdparty</url>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
</settings>
