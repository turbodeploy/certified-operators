<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <localRepository>${user.home}/.m2/repository</localRepository>
    <interactiveMode>false</interactiveMode>
    <offline>false</offline>
    <mirrors/>
    <proxies/>
    <profiles>
        <profile>
            <id>vmturbo</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>snapshots</id>
                    <name>snapshots</name>
                    <url>http://build.vmturbo.com:8081/nexus/content/repositories/snapshots</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>releases</id>
                    <name>Releases</name>
                    <url>http://build.vmturbo.com:8081/nexus/content/repositories/releases</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>vmturbo</id>
                    <name>vmturbo</name>
                    <url>http://build.vmturbo.com:8081/nexus/content/repositories/vmturbo</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>thirdparty</id>
                    <name>thirdparty</name>
                    <url>http://build.vmturbo.com:8081/nexus/content/repositories/thirdparty</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <!-- https://github.com/Swagger2Markup/swagger2markup/issues/300 -->
                <!--repository>
                    <id>spring-libs-milestone</id>
                    <url>https://repo.spring.io/milestone</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository -->
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>snapshots</id>
                    <name>snapshots</name>
                    <url>http://build.vmturbo.com:8081/nexus/content/repositories/snapshots</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>releases</id>
                    <name>Turbonomic Repository</name>
                    <url>http://build.vmturbo.com:8081/nexus/content/repositories/releases</url>
                </pluginRepository>
                <pluginRepository>
                    <id>vmturbo</id>
                    <name>Turbonomic Repository</name>
                    <url>http://build.vmturbo.com:8081/nexus/content/repositories/vmturbo</url>
                </pluginRepository>
                <pluginRepository>
                    <id>thirdparty</id>
                    <name>Turbonomic Repository</name>
                    <url>http://build.vmturbo.com:8081/nexus/content/repositories/thirdparty</url>
                </pluginRepository>
                <!-- https://github.com/Swagger2Markup/swagger2markup/issues/300 -->
                <!--pluginRepository>
                    <id>spring-plugins-release</id>
                    <url>https://repo.spring.io/plugins-release</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository -->
            </pluginRepositories>
        </profile>
    </profiles>
</settings>
