      "targets": [
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n    max(sub.name) as \"VM Name\",\n    max(sub.value) FILTER (WHERE sub.type = 'STORAGE_AMOUNT') AS \"Storage Used\",\n    avg(sub.value) FILTER (WHERE sub.type = 'STORAGE_ACCESS') AS \"Avg IOPS\",\n    max(sub.value) FILTER (WHERE sub.type = 'STORAGE_ACCESS') AS \"Max IOPS\",\n    avg(sub.value) FILTER (WHERE sub.type = 'STORAGE_LATENCY') AS \"Avg Latency\",\n    max(sub.value) FILTER (WHERE sub.type = 'STORAGE_LATENCY') AS \"Max Latency\"\nFROM (\n        select entity.oid as oid,\n               entity.name as name,\n               metric.type as type,\n               sum(metric.consumed) as value \n          from metric, entity\n         WHERE entity.type = 'VIRTUAL_MACHINE'\n           and entity.scope && (array[ [[Datastore]] ])\n           and entity.oid = metric.entity_oid and entity.hash = metric.entity_hash \n           and (metric.type = 'STORAGE_LATENCY' or metric.type = 'STORAGE_ACCESS') \n           and $__timeFilter(\"time\") and ($__timeFrom(),$__timeTo()) OVERLAPS (first_seen, last_seen)\n         GROUP BY time, 1, 2, 3\n       union\n        select entity.oid as oid,\n               max(entity.name) as name,\n               max(metric.type) as type,\n               sum(metric.consumed) as value \n          from metric, entity\n         WHERE entity.type = 'VIRTUAL_MACHINE' \n           and entity.scope && (array[ [[Datastore]] ])\n           and entity.oid = metric.entity_oid and entity.hash = metric.entity_hash and metric.type = 'STORAGE_AMOUNT'\n           and $__timeFilter(\"time\") and  ($__timeFrom(),$__timeTo()) OVERLAPS (first_seen, last_seen)\n         GROUP BY time, entity.oid\n) sub\ngroup by sub.oid",
