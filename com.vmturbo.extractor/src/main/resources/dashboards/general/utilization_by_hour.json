          "format": "time_series",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  date_trunc('hour', m.time) as \"time\",\n  avg(m.utilization) FILTER (WHERE m.type = 'VMEM') AS \"VMem\",\n  avg(m.utilization) FILTER (WHERE m.type = 'VCPU') AS \"VCPU\"\nFROM entity e\nJOIN metric m ON m.entity_oid = e.oid AND\n  m.entity_hash = e.hash AND\n  $__timeFilter(m.\"time\") AND\n  m.type IN ('VMEM', 'VCPU') AND\n  e.type = 'VIRTUAL_MACHINE'  AND\n  ($__timeFrom(),$__timeTo()) OVERLAPS (first_seen, last_seen)\nGROUP BY 1\nORDER BY 1",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "current"
                ],
                "type": "column"
              }
            ]
          ],
