        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  entity.name as \"Host\", \n  REGEXP_REPLACE(metric.type, 'Q([0-9]*)_VCPU', '\\1 vCPU') \"Queue Type\",\n  avg(metric.utilization) AS \"Avg CPU Ready\",\n  max(metric.utilization) AS \"Max CPU Ready\"\nFROM metric, entity\nWHERE\n  $__timeFilter(\"time\") AND\n  metric.type LIKE 'Q%_VCPU' AND\n  metric.entity_oid = entity.oid AND\n  metric.entity_hash = entity.hash AND\n  entity.type = 'PHYSICAL_MACHINE' AND \n  ($__timeFrom(),$__timeTo()) OVERLAPS (first_seen, last_seen) AND\n  CASE WHEN ARRAY[ [[Cluster]] ] = ARRAY[ '0' ] THEN true ELSE (entity.scope && ARRAY[ [[Cluster]] ]::bigint[]) END\nGroup by entity.oid, entity.name, 2\nORDER BY 3 desc",
