          "rawQuery": true,
          "rawSql": "SELECT current_vals.time as \"time\",\n       current_avg as \"Current Week\",\n       offset_avg as \"Previous Week\"\nFROM\n    (SELECT\n      date_trunc('day', m.time) as \"time\",\n      avg(m.utilization) AS current_avg\n    FROM entity e\n    JOIN metric m ON m.entity_oid = e.oid AND\n      m.entity_hash = e.hash AND\n      m.type = 'MEM' AND\n      e.type = 'PHYSICAL_MACHINE'  AND\n      $__timeFilter(m.\"time\") AND\n      ($__timeFrom(),$__timeTo()) OVERLAPS (first_seen, last_seen) AND\n      CASE WHEN ARRAY[ [[Group]] ] = ARRAY[ '0' ] THEN true ELSE (e.scope && ARRAY[ [[Group]] ]::bigint[]) END\n    GROUP BY 1) as current_vals\nJOIN\n    (SELECT\n      date_trunc('day', m.time + INTERVAL '7 DAY') as \"time\",\n      avg(m.utilization) AS offset_avg\n    FROM entity e\n    JOIN metric m ON m.entity_oid = e.oid AND\n      m.entity_hash = e.hash AND\n      m.type = 'MEM' AND\n      e.type = 'PHYSICAL_MACHINE'  AND\n      m.time BETWEEN ($__timeFrom()::TIMESTAMP - interval '7 day') AND ($__timeTo()::TIMESTAMP - interval '7 day') AND\n      ($__timeFrom()::TIMESTAMP - interval '7 day',$__timeTo()::TIMESTAMP - interval '7 day') OVERLAPS (first_seen, last_seen) AND\n      CASE WHEN ARRAY[ [[Group]] ] = ARRAY[ '0' ] THEN true ELSE (e.scope && ARRAY[ [[Group]] ]::bigint[]) END\n    GROUP BY 1) as offset_vals\nON offset_vals.time = current_vals.time\nORDER BY 1",
