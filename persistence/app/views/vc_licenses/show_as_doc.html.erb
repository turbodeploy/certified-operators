<% unless @embedded %>

<div class="buttons_etc buttons_etc_top">
<table width="100%" border="0" cellspacing="0" style="margin-top:-5px;margin-bottom:30px;">
<tr>
<td width="60%" valign="top">
<%= js_button_to 'Modify', :controller => 'vc_licenses', :action => 'edit', :id => @vc_license %> 
<select onchange="javascript: location = this[this.selectedIndex].value;">
    <option value="#">Select an action...</option>
    <option value="#">----------------</option>
    <option value="<%=url_for(:controller => 'vc_licenses', :action => 'show', :id => @vc_license.id)%>"> Normal Format </option>
    <option value="<%=url_for(:controller => 'vc_licenses', :action => 'show_as_doc', :id => @vc_license.id)%>"> Document Format </option>
    <option value="#">----------------</option>
    <option value="<%=url_for(:controller => 'vc_licenses', :action => 'list')%>"> <%="Table of Vc License".pluralize%> </option>
    <option value="<%=url_for(:controller => 'vc_licenses', :action => 'block_list')%>"> <%="List of Vc License".pluralize%> </option>
    <option value="#">----------------</option>
    <option value="<%=url_for(:controller => 'vc_licenses', :action => 'new')%>"> Add new Vc License </option>

    <option value="#">----------------</option>

<!-- BEGIN-UID.user_actions -->

<!-- END-UID.user_actions -->

    </select>
</td>
<td class="context" width="40%">
   <%=context_links(@vc_license)%>
</td>
</tr>
</table>
</div>

<% end %>

<%
   # default vis control to show all
   unless @show_item
     @show_item = {}
     @show_item.default = true
   end

   @ft_view_section = {}
%>


<div class="show_as_doc_view show_as_doc_view_vc_license">    <!-- show_as_doc_view -->

    <div class="show_doc_props show_doc_props_vc_license">   <!-- show_doc_props -->

      <p class="doc_prev_next_links">
      <%=link_to "previous", url_show_as_doc(@vc_license.prev_item), :title => "Previous Vc License", :style => "font-size:8pt;"%> 
      | <%=link_to "next", url_show_as_doc(@vc_license.next_item), :title => "Next Vc License", :style => "font-size:8pt;"%>
      </p>

<!-- BEGIN-UID.user_properties -->

<!-- END-UID.user_properties -->



<% @ft_view_section[:target] = capture { %>
<% if @show_item[:target] then %>

  <!-- Target -->

  <div class="doc_attr_group attr_group_vc_license_target title">     <!-- div10-b -->
    <div class="doc_attr_label"> Target </div>                   <!-- div11-b -->
    <div class="doc_attr_value single_line"> <%= auto_link(@vc_license.target.to_s) %>&nbsp;</div>   <!-- div12h -->
  </div>   <!-- div10 end -->
  <p class="spacer">&nbsp;</p>

<% end %>
<% } %>



<% @ft_view_section[:product] = capture { %>
<% if @show_item[:product] then %>

  <!-- Product -->

  <div class="doc_attr_group attr_group_vc_license_product ">     <!-- div10-b -->
    <div class="doc_attr_label"> Product </div>                   <!-- div11-b -->
    <div class="doc_attr_value single_line"> <%= auto_link(@vc_license.product.to_s) %>&nbsp;</div>   <!-- div12h -->
  </div>   <!-- div10 end -->

<% end %>
<% } %>



<% @ft_view_section[:capacity] = capture { %>
<% if @show_item[:capacity] then %>

  <!-- Capacity -->

  <div class="doc_attr_group attr_group_vc_license_capacity ">     <!-- div10-b -->
    <div class="doc_attr_label"> Capacity </div>                   <!-- div11-b -->
    <div class="doc_attr_value single_line"> <%= auto_link(@vc_license.capacity.to_s) %>&nbsp;</div>   <!-- div12h -->
  </div>   <!-- div10 end -->

<% end %>
<% } %>



<% @ft_view_section[:used] = capture { %>
<% if @show_item[:used] then %>

  <!-- Used -->

  <div class="doc_attr_group attr_group_vc_license_used ">     <!-- div10-b -->
    <div class="doc_attr_label"> Used </div>                   <!-- div11-b -->
    <div class="doc_attr_value single_line"> <%= auto_link(@vc_license.used.to_s) %>&nbsp;</div>   <!-- div12h -->
  </div>   <!-- div10 end -->

<% end %>
<% } %>

<% @ft_view_section_order = []
   @ft_view_section_order << :target
   @ft_view_section_order << :product
   @ft_view_section_order << :capacity
   @ft_view_section_order << :used
%>

<!-- BEGIN-UID.user_ordering -->

<!-- END-UID.user_ordering -->

<% for s in @ft_view_section_order %>
   <%= @ft_view_section[s].to_s %>
<% end %>

    </div>   <!-- show_doc_props -->

    <p class="spacer">&nbsp;</p>


<% @ft_view_section = {} %>

<% @ft_view_section_order = []
%>

<!-- BEGIN-UID.user_text_ordering -->

<!-- END-UID.user_text_ordering -->

<% for s in @ft_view_section_order %>
   <%= @ft_view_section[s].to_s %>
<% end %>

<div class="show_as_doc_related">

<!-- BEGIN-UID.user_related_items -->
<h3 class="rel_items_heading">Related Items</h3>
<!-- END-UID.user_related_items -->

<div class="show_hide_subtable_text">
<% listtext_op = session['list_text_toggles'] ? "show text in tables" : "hide text in tables" %>
<%= link_to listtext_op, {:controller => 'fast_ops', :action => 'toggle_list_text_toggle'}, :style => 'font-size:8pt;' %> |
<% listimage_op = session['list_image_toggles'] ? "show images in tables" : "hide images in tables" %>
<%= link_to listimage_op, {:controller => 'fast_ops', :action => 'toggle_list_image_toggle'}, :style => 'font-size:8pt;' %>
</div>


<% @ft_view_section = {} %>

<%     @ft_view_section_order = []
%>
<!-- BEGIN-UID.user_rel_ordering -->

<!-- END-UID.user_rel_ordering -->

<% for s in @ft_view_section_order do %>
   <%= @ft_view_section[s].to_s %>
<% end %>

</div> <!-- show_as_doc_related -->


</div>  <!-- show_as_doc_view -->

<!-- BEGIN-UID.userdivs -->

<!-- END-UID.userdivs -->

