    <option value="#">----------------</option>

<!-- BEGIN-UID.user_actions -->

<!-- END-UID.user_actions -->

    </select>
</td>
<td class="context" width="40%">
   <%=context_links(@report_subscription)%>
</td>
</tr>
</table>
</div>

<% end %>

<%
