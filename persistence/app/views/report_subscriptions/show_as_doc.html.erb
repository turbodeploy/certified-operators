<% unless @embedded %>

<div class="buttons_etc buttons_etc_top">
<table width="100%" border="0" cellspacing="0" style="margin-top:-5px;margin-bottom:30px;">
<tr>
<td width="60%" valign="top">
<%= js_button_to 'Modify', :controller => 'report_subscriptions', :action => 'edit', :id => @report_subscription %> 
<select onchange="javascript: location = this[this.selectedIndex].value;">
    <option value="#">Select an action...</option>
    <option value="#">----------------</option>
    <option value="<%=url_for(:controller => 'report_subscriptions', :action => 'show', :id => @report_subscription.id)%>"> Normal Format </option>
    <option value="<%=url_for(:controller => 'report_subscriptions', :action => 'show_as_doc', :id => @report_subscription.id)%>"> Document Format </option>
