<div id="add_new_to_report_subscriptions" style="float:right; width:60%;">
   <% dest = url_for :controller => 'standard_reports', :action => 'update_report_subscriptions', :id => @standard_report.id %>
   <%= form_tag( :controller => 'report_subscriptions', :action => 'new', :redirect => dest ) %>
       <%= submit_tag 'Add New...' %>
   </form> <!-- end form_tag -->
</div>

<div id="add_to_report_subscriptions_list">
   <%= form_tag( :controller => 'standard_reports', :action => :update_report_subscriptions, :id => @standard_report ) %>
       <%= submit_tag 'Add Selected' %>
       <p class="select_list">&nbsp;</p>
       <% for report_subscription in @report_subscriptions %>
           <p class="select_list"> <%= check_box_tag("report_subscription_ids[]",report_subscription.id) %> <%= report_subscription.sname %> </p>
       <% end %>
       <p class="select_list">&nbsp;</p>
       <%= submit_tag 'Add Selected' %>
   </form> <!-- end form_tag -->
</div>

